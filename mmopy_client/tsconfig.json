{
  "compilerOptions": {
      "module": "commonjs",
      "jsx": "react",
      "esModuleInterop": true,
      "target": "es6",
      "noImplicitAny": true,
      "moduleResolution": "node",
      "sourceMap": true,
      "rootDir": "src",
      "outDir": "build",
      "baseUrl": ".",
      "paths": {
          "*": [
              "../node_modules/*",
              "src/types/*"
          ]
      }
  },
  "include": [
      "src/**/*"
  ],
}
